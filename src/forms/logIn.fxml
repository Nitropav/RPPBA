        </Label>
        <Label layoutX="270.0" layoutY="80.0" text="Введите логин">
            <font>
                <Font size="14.0" />
            </font></Label>
        <Label layoutX="270.0" layoutY="160.0" text="Введите пароль">
            <font>
                <Font size="14.0" />
            </font></Label>
        <TextField fx:id="user" layoutX="250.0" layoutY="110.0" text="" />
        <PasswordField fx:id="password" layoutX="250.0" layoutY="190.0" text="" />
        <Button layoutX="70.0" layoutY="300.0" mnemonicParsing="false" onAction="#log" prefHeight="44.0" prefWidth="93.0" text="Войти" />
        <Button layoutX="411.0" layoutY="365.0" mnemonicParsing="false" onAction="#registerUser" prefHeight="44.0" prefWidth="160.0" text="Зарегистрироваться" />
